    "jest": "26.6.3",
    "jest-watch-typeahead": "0.6.1",
    "lint-staged": "10.5.4",
    "prettier": "2.2.1",
    "typescript": "4.2.3",
    "ts-jest": "26.5.3"
  },
  "babelMacros": {
    "twin": {
      "preset": "emotion"
